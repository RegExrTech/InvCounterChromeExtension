{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "InvCounter",
  "version": "1.17.0",
  "manifest_version": 2,
  "description": "Tells how many items are in stock at Funko-Shop.com, HotTopic.com, GalacticToys.com, Target.com, and Emp.co.uk",
  "homepage_url": "https://reddit.com/u/regexr",
  "background": {
    "scripts": [
      "jquery.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "InvCounter"
  },
  "icons": { 
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "permissions": [
    "https://shop.funko.com/*",
    "http://shop.funko.com/*",
    "https://www.hottopic.com/*",
    "http://www.hottopic.com/*",
    "https://www.boxlunch.com/*",
    "http://www.boxlunch.com/*",
    "https://www.emp.co.uk/*",
    "http://www.emp.co.uk/*",
    "https://www.target.com/*",
    "http://www.target.com/*"
  ]
}
